generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Aluno {
  matricula String @id @unique
  nome String
  nascimento String
  curso Curso @relation(fields: [cursoId], references: [id])
  cursoId Int
  @@map("alunos")
}

model Professor {
  id String @id @unique
  nome String
  disciplinas String
  @@map("professores")
}

model Disciplina {
  id String @id
  nome String
  curso String
  @@map("disciplinas")
}

model DisciplinasOnAlunos {
  
}

model Curso{
  id String @id @unique
  nome String
  alunos Aluno[]
  @@map("cursos")
}

model Nota {
  id Int @id @default(autoincrement())
  nota Decimal
  descricao String
  @@map("notas")
}


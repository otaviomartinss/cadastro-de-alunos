generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Aluno {
  matricula String @id @unique
  nome String
  nascimento String
  curso String
  @@map("alunos")
}

model Professor {
  id String @id @unique
  nome String
  disciplinas DisciplinasOnProfessores[]
  @@map("professores")
}

model Disciplina {
  id String @id
  nome String
  curso String
  professores DisciplinasOnProfessores[]
  @@map("disciplinas")
}

model DisciplinasOnProfessores {
  professor Professor @relation(fields: [professorId], references: [id])
  professorId String
  disciplina Disciplina @relation(fields: [disciplinaId], references: [id])
  disciplinaId String
  @@id([professorId, disciplinaId])
}

model Curso{
  id String @id @unique
  nome String
  @@map("cursos")
}

model Nota {
  id Int @id @default(autoincrement())
  nota Decimal
  descricao String
  @@map("notas")
}

